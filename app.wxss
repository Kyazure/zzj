/**app.wxss**/
page{
  height: 100%;
  /* background:linear-gradient(#ccffcc, #ccffcc, #99ffcc) ; */
  background: #f5f6f7;
}
.container {
  height: 100%;
  flex-direction: column;
  align-items: center;
  display: flex;
  justify-content: flex-start; 
  box-sizing: border-box;
  padding-top: 150rpx;
} 
pages {
  position: relative;
  z-index: 9999998;
  background: #fff;
}
page::after{
  content: '';
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 2rpx;
  background-color: rgb(96, 243, 11);
  z-index: 9999;
}
